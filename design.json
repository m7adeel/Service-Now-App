{
    "designSystem": {
        "name": "Service Booking App Design System",
        "version": "1.0",
        "description": "A comprehensive design system for service booking mobile applications",
        "colors": {
            "primary": {
                "blue": "#007AFF",
                "lightBlue": "#E3F2FD"
            },
            "secondary": {
                "orange": "#FF9500",
                "lightOrange": "#FFF3E0"
            },
            "neutral": {
                "white": "#FFFFFF",
                "lightGray": "#F8F9FA",
                "mediumGray": "#E5E5EA",
                "darkGray": "#8E8E93",
                "black": "#000000",
                "text": "#1C1C1E",
                "secondaryText": "#8E8E93"
            },
            "status": {
                "success": "#34C759",
                "warning": "#FF9500",
                "error": "#FF3B30",
                "info": "#007AFF"
            }
        },
        "typography": {
            "fontFamily": "SF Pro Display, -apple-system, BlinkMacSystemFont, sans-serif",
            "scales": {
                "h1": {
                    "fontSize": "28px",
                    "fontWeight": "700",
                    "lineHeight": "34px",
                    "letterSpacing": "0.36px"
                },
                "h2": {
                    "fontSize": "22px",
                    "fontWeight": "600",
                    "lineHeight": "28px",
                    "letterSpacing": "0.35px"
                },
                "h3": {
                    "fontSize": "20px",
                    "fontWeight": "600",
                    "lineHeight": "25px",
                    "letterSpacing": "0.38px"
                },
                "body": {
                    "fontSize": "17px",
                    "fontWeight": "400",
                    "lineHeight": "22px",
                    "letterSpacing": "-0.41px"
                },
                "bodyBold": {
                    "fontSize": "17px",
                    "fontWeight": "600",
                    "lineHeight": "22px",
                    "letterSpacing": "-0.41px"
                },
                "caption": {
                    "fontSize": "13px",
                    "fontWeight": "400",
                    "lineHeight": "18px",
                    "letterSpacing": "-0.08px"
                },
                "captionBold": {
                    "fontSize": "13px",
                    "fontWeight": "600",
                    "lineHeight": "18px",
                    "letterSpacing": "-0.08px"
                },
                "small": {
                    "fontSize": "11px",
                    "fontWeight": "400",
                    "lineHeight": "13px",
                    "letterSpacing": "0.07px"
                }
            }
        },
        "spacing": {
            "xs": "4px",
            "sm": "8px",
            "md": "16px",
            "lg": "24px",
            "xl": "32px",
            "xxl": "48px"
        },
        "borderRadius": {
            "none": "0px",
            "sm": "8px",
            "md": "12px",
            "lg": "16px",
            "xl": "20px",
            "full": "9999px"
        },
        "shadows": {
            "sm": "0 2px 4px rgba(0, 0, 0, 0.06)",
            "md": "0 4px 6px rgba(0, 0, 0, 0.07)",
            "lg": "0 10px 15px rgba(0, 0, 0, 0.1)",
            "card": "0 2px 8px rgba(0, 0, 0, 0.08)"
        },
        "components": {
            "buttons": {
                "primary": {
                    "backgroundColor": "#007AFF",
                    "color": "#FFFFFF",
                    "borderRadius": "12px",
                    "padding": "14px 24px",
                    "fontSize": "17px",
                    "fontWeight": "600",
                    "border": "none",
                    "minHeight": "50px"
                },
                "secondary": {
                    "backgroundColor": "#F2F2F7",
                    "color": "#007AFF",
                    "borderRadius": "12px",
                    "padding": "14px 24px",
                    "fontSize": "17px",
                    "fontWeight": "600",
                    "border": "none",
                    "minHeight": "50px"
                },
                "icon": {
                    "backgroundColor": "transparent",
                    "color": "#8E8E93",
                    "borderRadius": "8px",
                    "padding": "8px",
                    "fontSize": "16px",
                    "border": "none",
                    "minHeight": "32px",
                    "minWidth": "32px"
                }
            },
            "cards": {
                "service": {
                    "backgroundColor": "#FFFFFF",
                    "borderRadius": "16px",
                    "padding": "16px",
                    "boxShadow": "0 2px 8px rgba(0, 0, 0, 0.08)",
                    "border": "1px solid #F2F2F7",
                    "layout": {
                        "imageHeight": "120px",
                        "imageRadius": "12px",
                        "contentSpacing": "12px"
                    }
                },
                "profile": {
                    "backgroundColor": "#FFFFFF",
                    "borderRadius": "16px",
                    "padding": "20px",
                    "boxShadow": "0 2px 8px rgba(0, 0, 0, 0.08)",
                    "layout": {
                        "imageSize": "80px",
                        "imageRadius": "12px"
                    }
                }
            },
            "inputs": {
                "search": {
                    "backgroundColor": "#F2F2F7",
                    "borderRadius": "12px",
                    "padding": "12px 16px",
                    "fontSize": "17px",
                    "border": "none",
                    "minHeight": "44px",
                    "placeholder": {
                        "color": "#8E8E93"
                    }
                },
                "text": {
                    "backgroundColor": "#F2F2F7",
                    "borderRadius": "8px",
                    "padding": "12px 16px",
                    "fontSize": "17px",
                    "border": "1px solid #E5E5EA",
                    "minHeight": "44px"
                }
            },
            "navigation": {
                "header": {
                    "backgroundColor": "#FFFFFF",
                    "height": "88px",
                    "paddingTop": "44px",
                    "paddingHorizontal": "16px",
                    "borderBottom": "1px solid #E5E5EA",
                    "title": {
                        "fontSize": "17px",
                        "fontWeight": "600",
                        "color": "#1C1C1E"
                    }
                },
                "tabBar": {
                    "backgroundColor": "#FFFFFF",
                    "height": "83px",
                    "paddingBottom": "34px",
                    "paddingHorizontal": "16px",
                    "borderTop": "1px solid #E5E5EA",
                    "iconSize": "24px",
                    "fontSize": "11px"
                }
            },
            "ratings": {
                "star": {
                    "size": "16px",
                    "activeColor": "#FF9500",
                    "inactiveColor": "#E5E5EA",
                    "spacing": "2px"
                },
                "badge": {
                    "backgroundColor": "#FF9500",
                    "color": "#FFFFFF",
                    "borderRadius": "6px",
                    "padding": "2px 6px",
                    "fontSize": "11px",
                    "fontWeight": "600"
                }
            },
            "categories": {
                "item": {
                    "backgroundColor": "#F2F2F7",
                    "borderRadius": "16px",
                    "padding": "16px",
                    "minHeight": "80px",
                    "iconSize": "32px",
                    "fontSize": "13px",
                    "fontWeight": "600",
                    "active": {
                        "backgroundColor": "#007AFF",
                        "color": "#FFFFFF"
                    }
                }
            }
        },
        "layout": {
            "screenPadding": "16px",
            "sectionSpacing": "24px",
            "cardSpacing": "12px",
            "gridGap": "12px",
            "listItemSpacing": "16px"
        },
        "patterns": {
            "serviceCard": {
                "structure": [
                    "image (aspect ratio 3:2)",
                    "title (bodyBold)",
                    "subtitle (caption, secondaryText)",
                    "rating (stars + count)",
                    "price (bodyBold, primary color)",
                    "action button"
                ]
            },
            "profileHeader": {
                "structure": [
                    "back button",
                    "share button",
                    "profile image (large)",
                    "name (h2)",
                    "specialty + experience (caption)",
                    "location (caption, secondaryText)",
                    "rating display"
                ]
            },
            "bookingFlow": {
                "steps": [
                    "service selection",
                    "date/time selection",
                    "additional details",
                    "payment confirmation"
                ],
                "stepIndicator": {
                    "activeColor": "#007AFF",
                    "inactiveColor": "#E5E5EA",
                    "fontSize": "13px"
                }
            },
            "searchInterface": {
                "structure": [
                    "location selector (dropdown)",
                    "search bar",
                    "category icons grid (2x3)",
                    "nearby services section"
                ]
            }
        },
        "iconography": {
            "style": "SF Symbols / iOS system icons",
            "sizes": {
                "sm": "16px",
                "md": "24px",
                "lg": "32px"
            },
            "categories": {
                "builders": "hammer",
                "electrician": "bolt",
                "plumber": "drop",
                "handcraft": "wrench"
            }
        },
        "interactions": {
            "tapFeedback": {
                "scale": "0.95",
                "duration": "150ms",
                "opacity": "0.8"
            },
            "transitions": {
                "standard": "300ms ease-out",
                "quick": "150ms ease-out",
                "slow": "500ms ease-out"
            }
        },
        "responsive": {
            "breakpoints": {
                "mobile": "375px",
                "tablet": "768px"
            },
            "gridColumns": {
                "categories": 4,
                "services": 2
            }
        },
        "accessibility": {
            "minimumTapTarget": "44px",
            "contrastRatio": {
                "normal": "4.5:1",
                "large": "3:1"
            },
            "focusIndicator": {
                "color": "#007AFF",
                "width": "2px",
                "style": "solid"
            }
        },
        "usage": {
            "principles": [
                "Use consistent spacing based on 8px grid system",
                "Maintain visual hierarchy with typography scales",
                "Apply consistent border radius for cohesive feel",
                "Use primary blue for CTAs and active states",
                "Orange for ratings and highlights",
                "Gray scale for supporting content",
                "Cards should have subtle shadows and borders",
                "Icons should be consistent in style and sizing",
                "Maintain 44px minimum touch targets",
                "Use appropriate contrast ratios for accessibility"
            ],
            "doAndDont": {
                "do": [
                    "Use the defined color palette consistently",
                    "Apply proper spacing using the scale",
                    "Maintain consistent border radius",
                    "Use appropriate typography hierarchy",
                    "Include proper touch targets",
                    "Apply shadows consistently to cards"
                ],
                "dont": [
                    "Mix different corner radius values randomly",
                    "Use colors outside the defined palette",
                    "Create touch targets smaller than 44px",
                    "Apply inconsistent spacing",
                    "Use more than 3 font weights per screen"
                ]
            }
        }
    }
}